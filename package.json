{
  "name": "jquery-quiz-using-json",
  "version": "0.0.0-semantically-released",
  "description": "A jquery quiz for scoring multiple choice questions",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "npm run test",
    "watch:test": "npm t -- --watch",
    "test": "mocha test/quiz.spec.js --compilers js:babel-register",
    "cover": "nyc --reporter=lcov npm t",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.spec.js test",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "validate": "npm run cover && npm run check-coverage",
    "setup": "npm install && npm run validate",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Matt-Webb/jquery-quiz-using-json.git"
  },
  "keywords": [
    "javascript-quiz",
    "quiz",
    "json"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Matthew D Webb <matt.d.webb@icloud.com> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Matt-Webb/jquery-quiz-using-json/issues"
  },
  "homepage": "https://github.com/Matt-Webb/jquery-quiz-using-json#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.11.6",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "commitizen": "2.8.5",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.3.2",
    "json-loader": "0.5.4",
    "mocha": "3.0.1",
    "npm-run-all": "2.3.0",
    "nyc": "7.1.0",
    "rimraf": "2.5.4",
    "semantic-release": "^4.2.1",
    "standard": "^9.0.0",
    "webpack": "1.13.1"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
